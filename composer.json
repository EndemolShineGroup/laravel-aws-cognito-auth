{
  "name": "endemol/awsCognitoAuth",
  "description": "An authentication driver for Laravel for authenticating users in AWS Cognito User Pools",
  "keywords": ["php", "laravel", "auth", "aws", "cognito"],
  "license": "MIT",
  "authors": [
    {
      "name": "Arran Jacques",
      "email": "arranjacques@gmail.com"
    }
  ],
  "require": {
    "php": ">= 5.6.4",
    "aws/aws-sdk-php-laravel": "^3.1",
    "nesbot/carbon": "~1.20",
    "ramsey/uuid": "^3.7"
  },
  "autoload": {
    "psr-0": {
      "Endemol\\AwsCognitoAuth": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Endemol\\AwsCognitoAuth\\CognitoUserServiceProvider",
        "Endemol\\AwsCognitoAuth\\CognitoAuthServiceProvider"
      ]
    }
  }
}
